{
  "name": "Autojump Extension",
  "version": "3.2",
  "description": "The extension made by zghaia@gmail.com for auto jumping from one site to another site",
  "browser_action": {
    "default_title": "Autojump Extension",
    "default_icon": "logo.png"
  },
  "icons": {
      "128"	: "logo.png",
      "48"	: "logo.png",
	  	"16"	: "logo.png"
  },
  "permissions": [
  		"tabs", 
  		"http://*/*"
  ],
  "background_page": "background.html",
  "options_page": "options.html",
  "update_url": "http://zghaia-autojump-extension.googlecode.com/files/update.xml"
}